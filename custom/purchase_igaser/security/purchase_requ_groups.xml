<odoo>
    <record id="group_stock_picking_delivery_slip" model="res.groups">
        <field name="name">Inventaire/ison</field>
    </record>

    <record id="purchase_request_group_to_approve" model="res.groups">
        <field name="name">Valider</field>
        <field name="type">button</field>
        <field name="model_id" ref="purchase_request.model_purchase_request"/>
    </record>

    <record id="purchase_request_group_approved" model="res.groups">
        <field name="name">Approuver</field>
        <field name="type">button</field>
        <field name="model_id" ref="purchase_request.model_purchase_request"/>
    </record>

    <record id="group_siege" model="res.groups">
        <field name="name">Siège</field>
    </record>

    <record id="group_siege_pr" model="res.groups">
        <field name="name">Siège / Principal</field>
        <field name="implied_ids" eval="[(4, ref('purchase_igaser.group_siege'))]"/>
    </record>

    <record id="group_siege_fg" model="res.groups">
        <field name="name">Siège / Frais généraux</field>
        <field name="implied_ids" eval="[(4, ref('purchase_igaser.group_siege'))]"/>
    </record>

    <record id="group_siege_lg" model="res.groups">
        <field name="name">Siège / Logistique</field>
        <field name="implied_ids" eval="[(4, ref('purchase_igaser.group_siege'))]"/>
    </record>
</odoo>