<odoo>
    <record id="view_backorder_confirmation_inherit_custom" model="ir.ui.view">
        <field name="name">stock.backorder.confirmation.inherit.custom</field>
        <field name="model">stock.backorder.confirmation</field>
        <field name="inherit_id" ref="stock.view_backorder_confirmation"/>
        <field name="arch" type="xml">
            <xpath expr="//footer//button[@name='process_cancel_backorder']" position="attributes">
                <attribute name="name">open_process_cancel_backorder_wizard</attribute>
            </xpath>
        </field>
    </record>

