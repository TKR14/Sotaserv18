<odoo>
    <record id="dashboard_view" model="ir.ui.view">
        <field name="name">dashboard.view</field>
        <field name="model">dashboard</field>
        <field name="arch" type="xml">
            <list>
                <field name="site_id"/>
                <field name="tab"/>
                <field name="product_id"/>
                <field name="initial_quantity"/>
                <field name="initial_price_unit"/>
                <field name="initial_budget"/>
                <field name="consumed_quantity"/>
                <field name="consumed_amount"/>
                <field name="remaining_quantity"/>
                <field name="remaining_price"/>
                <field name="remaining_amoun"/>
                <field name="difference"/>
            </list>
        </field>
    </record>

    <record id="dashboard_action" model="ir.actions.act_window">
        <field name="name">Dashboard</field>
        <field name="res_model">dashboard</field>
        <field name="view_mode">list</field>
    </record>
</odoo>