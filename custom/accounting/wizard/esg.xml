<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_esg_wizard" model="ir.ui.view">
		<field name="name">esg.wizard.view</field>
		<field name="model">esg.wizard</field>
		<field name="arch" type="xml">
			<form>
				<group>
					<group>
						<field name="from_date" />
						<field name="to_date" />
					</group>
					<group>
						<field name="entries" widget="radio" />
