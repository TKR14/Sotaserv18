				<footer>
					<button string="Imprimer" class="btn-primary" name="print_data" type="object" />
					<button string="Annuler" class="btn-secondary" special="cancel" />
				</footer>
			</form>
		</field>
	</record>
	<record id="action_esg_wizard" model="ir.actions.act_window">
		<field name="name">ESG</field>
