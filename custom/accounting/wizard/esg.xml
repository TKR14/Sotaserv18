		<field name="res_model">esg.wizard</field>
		<field name="view_mode">form</field>
		<field name="view_id" ref="view_esg_wizard" />
		<field name="target">new</field>
	</record>
	<menuitem id="menu_esg_wizard" name="ESG" sequence="21"
